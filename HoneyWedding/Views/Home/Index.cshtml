<div class="honeyContentBlock mdl-color--deep-purple-100 argyle-overlay" id="whenWhere">
    <h1 class="mdl-color-text--primary-dark">When & Where</h1>
    <div class="mdl-grid">
        <div class="mdl-cell mdl-cell--4-col mdl-cell--4-col-tablet mdl-cell--4-col-phone">
            <div class="mdl-card mdl-shadow--2dp">
                <div class="mdl-card__title" style="background: url('../../Content/img/when_where.jpg') center; background-size: cover; height: 200px;">
                    <div class="mdl-card__title-text">Details</div>
                </div>
                <div class="mdl-card__supporting-text">
                    <table class="table-list" border="0" cellpadding="0" cellspacing="0">
                        <tr>
                            <td class="table-list--term">Date</td>
                            <td>
                                October 1, 2016
                            </td>
                        </tr>
                        <tr>
                            <td class="table-list--term">Time</td>
                            <td>
