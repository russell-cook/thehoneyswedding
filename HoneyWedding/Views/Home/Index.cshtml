@section scripts {
    <script src="~/Content/greensock-js/src/minified/TweenMax.min.js"></script>
    <script src="~/Content/greensock-js/src/minified/plugins/ScrollToPlugin.min.js"></script>
    <script src="~/Content/scrollmagic/minified/ScrollMagic.min.js"></script>
    <script src="~/Content/scrollmagic/minified/plugins/animation.gsap.min.js"></script>
    <script src="~/Content/slick/slick.min.js"></script>
    <script type="text/javascript">
        var controller = new ScrollMagic.Controller({ globalSceneOptions: {} });
        var sceneOurStory;
        var sceneOurFamilies;
        var sceneWhenWhere;
        var sceneLisaChase;
        var sceneRussChase;

        var bigHoneyBikeChaseFlightPath;
        var bigHoneyBikeChaseTween;
        var bigHoneyBikeChaseScene;
        var littleHoneyBikeChaseFlightPath;
        var littleHoneyBikeChaseTween;
        var littleHoneyBikeChaseScene;
        var bigHoneyFootChaseFlightPath;
        var bigHoneyFootChaseTween;
        var bigHoneyFootChaseScene;
        var littleHoneyFootChaseFlightPath;
        var littleHoneyFootChaseTween;
        var littleHoneyFootChaseScene;

        function initScrollMagic() {
            // build nav tab scenes
            sceneOurStory = new ScrollMagic.Scene({ triggerElement: "#ourStory" })
                            .setClassToggle("#menuOurStory", "is-active")
                            .addTo(controller);
            sceneOurFamilies = new ScrollMagic.Scene({ triggerElement: "#ourFamilies" })
                            .setClassToggle("#menuOurFamilies", "is-active")
                            .addTo(controller);
            sceneWhenWhere = new ScrollMagic.Scene({ triggerElement: "#whenWhere" })
                            .setClassToggle("#menuWhenWhere", "is-active")
                            .addTo(controller);

            // build animation scenes
            sceneLisaChase = new ScrollMagic.Scene({ triggerElement: "#viewportChase", triggerHook: 0.8 })
                            //.setPin("#lisaChaseContainer")
                            .addTo(controller);

            sceneRussChase = new ScrollMagic.Scene({ triggerElement: "#viewportChase", triggerHook: 0.8 })
                            //.setPin("#russChaseContainer")
                            .addTo(controller);
            sceneLisaBikeChase = new ScrollMagic.Scene({ triggerElement: "#viewportBikeChase", triggerHook: 0.8 })
                            //.setPin("#lisaChaseContainer")
                            .addTo(controller);

            sceneRussBikeChase = new ScrollMagic.Scene({ triggerElement: "#viewportBikeChase", triggerHook: 0.8 })
                            //.setPin("#russChaseContainer")
                            .addTo(controller);
        }

        function calcScrollMagic() {
            // calculate and set triggerHook (i.e. vertical position of trigger)
            // this should place triggerHook exactly at the bottom of the headerRow (i.e. menu bar)
            var headerHeight = $('#headerRow').height();
            var windowHeight = $(window).height()
            var triggerHookVal = (Math.ceil((headerHeight / windowHeight) * 100) / 100).toFixed(2); // calculate what percentage of window height is occupied by menu bar, round up to two decimal places to add buffer for small screen heights
            sceneOurStory.triggerHook(triggerHookVal);
            sceneOurFamilies.triggerHook(triggerHookVal);
            sceneWhenWhere.triggerHook(triggerHookVal);

            // calculate duration (i.e. height) of each honeyContentBlock scene
            var ourStoryHeight = $('#ourStory').height();
            sceneOurStory.duration(ourStoryHeight);
            var ourFamiliesHeight = $('#ourFamilies').height();
            sceneOurFamilies.duration(ourFamiliesHeight);
            var whenWhereHeight = $('#whenWhere').height();
            sceneWhenWhere.duration(whenWhereHeight);

            // calculate animation durations
            var viewportDuration = windowHeight - headerHeight;
            sceneLisaChase.duration(viewportDuration * 1.3);
            sceneRussChase.duration(viewportDuration * 1.4);
            sceneRussChase.offset(viewportDuration / 3); // this makes Russ lag behind Lisa
            sceneRussBikeChase.duration(viewportDuration * 1.3);
            sceneLisaBikeChase.duration(viewportDuration * 1.4);
            sceneLisaBikeChase.offset(viewportDuration / 2.5); // this makes Lisa lag behind Russ

            // calculate max-height of animation images
            $('.animationChaseImage, .animationBikeChaseImage').css({ 'max-height': viewportDuration / 2 });

            // calculate chase animation path
            var chaseImageCssLeftOffset = 300;
            var chaseWindowWidth = $(window).width() + chaseImageCssLeftOffset;

            chaseFlightPath = {
                entry: {
                    curviness: 1.25,
                    //autoRotate: true,
                    values: [
                            { x: 0, y: 0 },
                            { x: chaseWindowWidth * 0.1, y: 0 },
                            { x: chaseWindowWidth * 0.2, y: 0 },
                            { x: chaseWindowWidth * 0.3, y: viewportDuration * 0.1 }
                    ]
                },
                looping: {
                    curviness: 1.25,
                    //autoRotate: true,
                    values: [
                            { x: chaseWindowWidth * 0.4, y: viewportDuration * 0.2 },
                            { x: chaseWindowWidth * 0.5, y: viewportDuration * 0.3 },
                            { x: chaseWindowWidth * 0.6, y: viewportDuration * 0.4 }
                    ]
                },
                leave: {
                    curviness: 1.25,
                    //autoRotate: true,
                    values: [
                            { x: chaseWindowWidth * 0.8, y: viewportDuration * 0.5 },
                            { x: chaseWindowWidth * 0.9, y: viewportDuration * 0.7 },
                            { x: chaseWindowWidth, y: viewportDuration * 0.9 }
                    ]
                }
            };

            lisaChaseTween = new TimelineMax()
                .add(TweenMax.to($("#lisaChase"), 1, { css: { bezier: chaseFlightPath.entry }, ease: Power1.easeInOut }))
                .add(TweenMax.to($("#lisaChase"), 2, { css: { bezier: chaseFlightPath.looping }, ease: Power1.easeInOut }))
                .add(TweenMax.to($("#lisaChase"), 1, { css: { bezier: chaseFlightPath.leave }, ease: Power1.easeInOut }));

            russChaseTween = new TimelineMax()
                .add(TweenMax.to($("#russChase"), 1, { css: { bezier: chaseFlightPath.entry }, ease: Power1.easeInOut }))
                .add(TweenMax.to($("#russChase"), 2, { css: { bezier: chaseFlightPath.looping }, ease: Power1.easeInOut }))
                .add(TweenMax.to($("#russChase"), 1, { css: { bezier: chaseFlightPath.leave }, ease: Power1.easeInOut }));

            sceneLisaChase.setTween(lisaChaseTween);
            sceneRussChase.setTween(russChaseTween);

            // calculate bike chase animation path
            var bikeChaseImageCssRightOffset = 400;
            var bikeChaseWindowWidth = 0 - $(window).width() - bikeChaseImageCssRightOffset;

            bikeChaseFlightPath = {
                entry: {
                    curviness: 1.25,
                    //autoRotate: true,
                    values: [
                            { x: 0, y: 0 },
                            { x: bikeChaseWindowWidth * 0.1, y: 0 },
                            { x: bikeChaseWindowWidth * 0.2, y: 0.1 },
                            { x: bikeChaseWindowWidth * 0.3, y: viewportDuration * 0.2 }
                    ]
                },
                looping: {
                    curviness: 1.25,
                    //autoRotate: true,
                    values: [
                            { x: bikeChaseWindowWidth * 0.4, y: viewportDuration * 0.3 },
                            { x: bikeChaseWindowWidth * 0.5, y: viewportDuration * 0.4 },
                            { x: bikeChaseWindowWidth * 0.6, y: viewportDuration * 0.5 }
                    ]
                },
                leave: {
                    curviness: 1.25,
                    //autoRotate: true,
                    values: [
                            { x: bikeChaseWindowWidth * 0.8, y: viewportDuration * 0.6 },
                            { x: bikeChaseWindowWidth * 0.9, y: viewportDuration * 0.7 },
                            { x: bikeChaseWindowWidth, y: viewportDuration * 0.8 }
                    ]
                }
            };

            lisaBikeChaseTween = new TimelineMax()
                .add(TweenMax.to($("#lisaBikeChase"), 1, { css: { bezier: bikeChaseFlightPath.entry }, ease: Power1.easeInOut }))
                .add(TweenMax.to($("#lisaBikeChase"), 2, { css: { bezier: bikeChaseFlightPath.looping }, ease: Power1.easeInOut }))
                .add(TweenMax.to($("#lisaBikeChase"), 1, { css: { bezier: bikeChaseFlightPath.leave }, ease: Power1.easeInOut }));

            russBikeChaseTween = new TimelineMax()
                .add(TweenMax.to($("#russBikeChase"), 1, { css: { bezier: bikeChaseFlightPath.entry }, ease: Power1.easeInOut }))
                .add(TweenMax.to($("#russBikeChase"), 2, { css: { bezier: bikeChaseFlightPath.looping }, ease: Power1.easeInOut }))
                .add(TweenMax.to($("#russBikeChase"), 1, { css: { bezier: bikeChaseFlightPath.leave }, ease: Power1.easeInOut }));

            sceneLisaBikeChase.setTween(lisaBikeChaseTween);
            sceneRussBikeChase.setTween(russBikeChaseTween);


        }

        function initMap() {
            // Create a map object and specify the DOM element for display.
            var map = new google.maps.Map(document.getElementById('map'), {
                center: { lat: -34.397, lng: 150.644 },
                zoom: 8
            });
        }

        // init sliders
        function initSliders() {
            $('.slider__russ_born').slick({
                dots: true,
                lazyLoad: 'progressive'
            });

            $('.slider__lisa_born').slick({
                dots: true,
                lazyLoad: 'progressive'
            });

        }

    </script>

}

<script>
@section scriptWindowLoad {
    // initialize ScrollMagic, add event listener to re-calculate scrolling/animation values on resize
    initScrollMagic();
    calcScrollMagic();
    $(window).resize(calcScrollMagic);

    // initialize Slick image sliders
    initSliders();

    // enable automatic scrolling when clicking on an anchor link
    $(document).on("click", "a[href*='#']", function (e) {
        var hrefValue = $(this).attr("href");
        var id = hrefValue.substr(hrefValue.indexOf('#'), hrefValue.length);
        if ($(id).length > 0) {
            e.preventDefault();

            // trigger scroll
            var scrollLocation = $('.mdl-layout__content').scrollTop() + $(id).position().top;
            TweenMax.to('.mdl-layout__content', 1.5, { scrollTo: { y: scrollLocation }, ease: Power2.easeInOut });

            // if supported by the browser we can even update the URL.
            if (window.history && window.history.pushState) {
                history.pushState("", document.title, id);
            }
        }
    });

    // if loaded with # link, scroll to anchor
    if (window.location.hash) {
        var hrefValue = window.location.href;
        var id = hrefValue.substr(hrefValue.indexOf('#'), hrefValue.length);
        if ($(id).length > 0) {

            // trigger scroll
            var scrollLocation = $('.mdl-layout__content').scrollTop() + $(id).position().top;
            TweenMax.to('.mdl-layout__content', 1.5, { scrollTo: { y: scrollLocation }, ease: Power2.easeInOut });
        }
    }

    // load google map
    $('#map').attr('src', 'https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d118600.49331621807!2d-120.80860747195742!3d38.63542020196392!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x809a429b8410230b%3A0x83e40f62a39a4b29!2sMastroserio+Winery+WIne!5e0!3m2!1sen!2sus!4v1458862932354');

    }
</script>
