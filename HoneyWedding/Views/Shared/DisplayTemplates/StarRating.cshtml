@model int

@{ 
    <span class="star-rating">
        @for (int i = 0; i < Model; i++)
        {
            <i class="material-icons mdl-color-text--amber-500">star</i>
        }
        @for (int i = 5; i > Model; i--)
        {
            <i class="material-icons mdl-color-text--amber-500">star_border</i>
        }
    </span>
}

